<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.adminarea_realfood">
    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the "MyLocation" functionality.
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AdminArea_RealFood">
        <activity android:name=".ThanhToanActivity" android:label="Thanh Toán" android:parentActivityName=".Screen.TrangChu "></activity>
        <!--
             The API key for Google Maps-based APIs is defined as a string resource.
             (See the file "res/values/google_maps_api.xml").
             Note that the API key is linked to the encryption key used to sign the APK.
             You need a different API key for each encryption key, including the release key that is used to
             sign the APK for publishing.
             You can define the keys for the debug and release targets in src/debug/ and src/release/.
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".Map.MapsActivity"
            android:label="@string/title_activity_maps"
            android:parentActivityName=".Screen.TaoTaiKhoanShipper" />
        <activity
            android:name=".Screen.DoiMatKhau"
            android:label="@string/doimatkhau"
            android:parentActivityName=".Screen.TrangChu" />
        <activity
            android:name=".Screen.DoanhThuCuaHangActivity"
            android:label="Doanh thu cửa hàng"
            android:parentActivityName=".Screen.TrangChu" />
        <activity
            android:name=".Screen.DoanhThuHeThong"
            android:label="Doanh thu hệ thống"
            android:parentActivityName=".Screen.TrangChu" />
        <activity
            android:name=".Screen.ThongKeShipper"
            android:label="Thống kê"
            android:parentActivityName=".Screen.DanhSachThongKeShipper" />
        <activity
            android:name=".Screen.ThongKe_CuaHang_Activity"
            android:label="Thống kê"
            android:parentActivityName=".Screen.DoanhThuCuaHangActivity" />
        <activity
            android:name=".Screen.DanhSachThongKeShipper"
            android:label="Thống kê shipper"
            android:parentActivityName=".Screen.TrangChu" />
        <activity android:name=".Screen.XuLyShipper" />
        <activity
            android:name=".Screen.XuLyShop"
            android:label="Xử lý cửa hàng"
            android:parentActivityName=".Screen.ThongTinChiTietShop" />
        <activity
            android:name=".Screen.TaiKhoanNganHangAdmin"
            android:label="Tài khoản ngân hàng"
            android:parentActivityName=".Screen.TrangChu" />
        <activity
            android:name=".Screen.ThongTinChiTietKhachHang"
            android:label="Thông tin chi tiết khách hàng"
            android:parentActivityName=".Screen.DanhSachKhachHang" />
        <activity
            android:name=".Screen.DanhSachKhachHang"
            android:label="Danh sách khách hàng"
            android:parentActivityName=".Screen.TrangChu" />
        <activity
            android:name=".Screen.SuaLoaiSanPham"
            android:label="Sửa loại sản phẩm"
            android:parentActivityName=".Screen.DanhSachLoaiSanPham" />
        <activity
            android:name=".Screen.SuaTaiKhoanAdmin"
            android:label="Sửa tài khoản"
            android:parentActivityName=".Screen.TrangChu" />
        <activity
            android:name=".Screen.ThongTinChiTietShop"
            android:label="Thông tin chi tiết cửa hàng"
            android:parentActivityName=".Screen.DanhSachShop" />
        <activity
            android:name=".Screen.DanhSachShop"
            android:label="Danh sách shop"
            android:parentActivityName=".Screen.TrangChu" />
        <activity android:name=".Screen.DangNhap">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Screen.QuenMatKhau" />
        <activity android:name=".Screen.TrangChu" />
        <activity
            android:name=".Screen.TaoTaiKhoanShipper"
            android:label="Tạo tài khoản shipper"
            android:parentActivityName=".Screen.DanhSachShipper" />
        <activity
            android:name=".Screen.DanhSachShipper"
            android:label="Danh sách shipper"
            android:parentActivityName=".Screen.TrangChu" />
        <activity
            android:name=".Screen.DanhSachLoaiSanPham"
            android:label="Loại sản phẩm"
            android:parentActivityName=".Screen.TrangChu" />
        <activity
            android:name=".Screen.TaoLoaiSanPham"
            android:label="Tạo loại sản phẩm"
            android:parentActivityName=".Screen.DanhSachLoaiSanPham" />
        <activity
            android:name=".Screen.ThongTinChiTietShipper"
            android:label="Thông tin chi tiết shipper"
            android:parentActivityName=".Screen.DanhSachShipper" />

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="${applicationId}.com.vansuita.pickimage.provider"
            android:exported="false"
            android:grantUriPermissions="true"
            tools:replace="android:authorities">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/picker_provider_paths" />
        </provider>
    </application>

</manifest>